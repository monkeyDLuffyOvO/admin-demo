div\n                  class=\"link-item\"\n                  v-for=\"(item, index) in urlList\"\n                  :key=\"index\"\n          >\n            <div class=\"name\">{{ item.name }}</div>\n            <div class=\"link-txt\">地址：{{ item.url }}</div>\n            <div class=\"params\">\n              <span class=\"txt\">参数：</span>\n              <span>{{ item.parameter }}</span>\n            </div>\n            <div class=\"lable\">\n              <p class=\"txt\">例如：{{ item.example }}</p>\n              <Button\n                      size=\"small\"\n                      v-clipboard:copy=\"item.example\"\n                      v-clipboard:success=\"onCopy\"\n                      v-clipboard:error=\"onError\"\n              >复制\n              </Button\n              >\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- 中间 -->\n      <div\n              class=\"wrapper-con\"\n              style=\"\n          flex: 1;\n          background: #f0f2f5;\n          display: flex;\n          justify-content: center;\n          padding-top: 20px;\n          height: 100%;\n        \"\n      >\n        <div class=\"content\" style=\"display: flex;justify-content: center;\">\n          <div\n                  class=\"contxt\"\n                  style=\"\n              display: flex;\n              flex-direction: column;\n              overflow: hidden;\n              height: 100%;\n            \"\n          >\n            <div class=\"overflowy\">\n              <div class=\"picture\"><img src=\"@/assets/images/electric.png\"></div>\n              <div\n                      class=\"page-title\"\n                      :class=\"{ on: activeIndex == -100 }\"\n                      @click=\"showTitle\"\n              >\n                {{ titleTxt }}\n                <div class=\"delete-box\"></div>\n                <div class=\"handle\"></div>\n              </div>\n            </div>\n            <div class=\"scrollCon\">\n              <div style=\"width: 460px;margin: 0 auto\">\n                <div class=\"scroll-box\" :style=\"'height:'+ rollHeight + 'px;'\">\n                  <draggable\n                          class=\"dragArea list-group\"\n                          :list=\"mConfig\"\n                          group=\"people\"\n                          @change=\"log\"\n                          filter=\".top\"\n                          :move=\"onMove\"\n                          animation=\"300\"\n                  >\n                    <div\n                            class=\"mConfig-item\"\n                            :class=\"{\n                    on: activeIndex == key,\n                    top: item.name == 'search_box' || item.name == 'nav_bar',\n                  }\"\n                            v-for=\"(item, key) in mConfig\"\n                            :key=\"key\"\n                            @click.stop=\"bindconfig(item, key)\"\n                    >\n                      <component\n                              :is=\"item.name\"\n                              ref=\"getComponentData\"\n                              :configData=\"propsObj\"\n                              :index=\"key\"\n                              :num=\"item.num\"\n                      ></component>\n                      <div class=\"delete-box\">\n                        <div class=\"handleType\">\n                          <div class=\"iconfont iconshanchu2\" @click.stop=\"bindDelete(item, key)\"></div>\n                          <div class=\"iconfont iconfuzhi\" @click.stop=\"bindAddDom(item, 0, key)\"></div>\n                          <div class=\"iconfont iconshangyi\" :class=\"key===0?'on':''\" @click.stop=\"movePage(item, key, 1)\"></div>\n                          <div class=\"iconfont iconxiayi\" :class=\"key===mConfig.length-1?'on':''\" @click.stop=\"movePage(item, key, 0)\"></div>\n                        </div>\n                      </div>\n                      <div class=\"handle\"></div>\n                    </div>\n                  </draggable>\n                </div>\n              </div>\n            </div>\n            <div class=\"overflowy\">\n              <div\n                      class=\"page-foot\"\n                      @click=\"showFoot\"\n                      :class=\"{ on: activeIndex == -101 }\"\n              >\n                <footPage></footPage>\n                <div class=\"delete-box\"></div>\n                <div class=\"handle\"></div>\n              </div>\n            </div>\n          </div>\n          <div class=\"defaultData\" v-if=\"pageId !==0\">\n            <div class=\"data\" @click=\"setmoren\">设置默认</div>\n            <div class=\"data\" @click=\"getmoren\">恢复默认</div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 右侧 -->\n      <div class=\"right-box\">\n        <div class=\"mConfig-item\" v-for=\"(item, key) in rConfig\" :key=\"key\">\n          <div class=\"title-bar\">{{ item.cname }}</div>\n          <component\n                  :is=\"item.configName\"\n                  @config=\"config\"\n                  :activeIndex=\"activeIndex\"\n                  :num=\"item.num\"\n                  :index=\"key\"\n          ></component>\n        </div>\n      </div>\n    </div>\n  </Card>\n  <!--<div class=\"foot-box\">-->\n  <!--<Button @click=\"reast\">重置</Button>-->\n  <!--<Button type=\"primary\" @click=\"saveConfig\" :loading=\"loading\"-->\n  <!--&gt;保存-->\n  <!--</Button-->\n  <!--&gt;-->\n  <!--</div>-->\n</div>\n",null]}