Width\"\n                    :label-position=\"labelPosition\"\n                    @submit.native.prevent\n            >\n                <FormItem label=\"时间选择：\">\n                    <DatePicker\n                            :editable=\"false\"\n                            @on-change=\"onchangeTime\"\n                            :value=\"timeVal\"\n                            format=\"yyyy/MM/dd\"\n                            type=\"datetimerange\"\n                            placement=\"bottom-start\"\n                            placeholder=\"自定义时间\"\n                            class=\"input-width\"\n                            :options=\"options\"\n                    ></DatePicker>\n                </FormItem>\n\n                <FormItem label=\"评价状态：\">\n                    <Select\n                            v-model=\"formValidate.is_reply\"\n                            placeholder=\"请选择\"\n                            clearable\n                            @on-change=\"userSearchs\"\n                            class=\"input-add\"\n                    >\n                        <Option value=\"1\">已回复</Option>\n                        <Option value=\"0\">待回复</Option>\n                    </Select>\n                </FormItem>\n\n                <FormItem label=\"视频信息：\" label-for=\"keyword\">\n                    <Input\n                            size=\"default\"\n                            enter-button\n                            placeholder=\"请输入视频信息\"\n                            clearable\n                            v-model=\"formValidate.keyword\"\n                            class=\"input-add\"\n                    />\n                    <Button type=\"primary\" @click=\"userSearchs\">查询</Button>\n                </FormItem>\n            </Form>\n        </div>\n    </Card>\n\n    <Card :bordered=\"false\" dis-hover class=\"ivu-mt\">\n        <!-- 相关操作 -->\n        <Row type=\"flex\">\n            <Col v-bind=\"grid\">\n                <Button\n                        type=\"primary\"\n                        @click=\"add\"\n                >添加虚拟评论</Button\n                >\n            </Col>\n        </Row>\n        <!-- 商品评论表格 -->\n        <Table\n                ref=\"table\"\n                :columns=\"columns\"\n                :data=\"tableList\"\n                class=\"ivu-mt\"\n                :loading=\"loading\"\n                no-data-text=\"暂无数据\"\n                no-filtered-data-text=\"暂无筛选结果\"\n        >\n            <template slot-scope=\"{ row }\" slot=\"info\">\n                <div class=\"imgPic acea-row row-middle\">\n                    <viewer>\n                        <div class=\"pictrue\"><img v-lazy=\"row.video.image\" /></div>\n                    </viewer>\n                    <div class=\"info line2\">{{ row.video.desc }}</div>\n                </div>\n            </template>\n            <template slot-scope=\"{ row }\" slot=\"reply\">\n                <Tooltip max-width=\"200\" placement=\"bottom\">\n                    <span class=\"line2\">{{row.reply}}</span>\n                    <p slot=\"content\">{{row.reply}}</p>\n                </Tooltip>\n            </template>\n            <template slot-scope=\"{ row }\" slot=\"add_time\">\n                {{row.add_time | formatDate}}\n            </template>\n            <template slot-scope=\"{ row, index }\" slot=\"action\">\n                <a @click=\"seeReply(row)\">查看</a>\n                <Divider type=\"vertical\" />\n                <a @click=\"reply(row)\">回复</a>\n                <Divider type=\"vertical\" />\n                <a @click=\"del(row, '删除评论', index)\">删除</a>\n            </template>\n        </Table>\n        <div class=\"acea-row row-right page\">\n            <Page\n                    :total=\"total\"\n                    :current=\"formValidate.page\"\n                    show-elevator\n                    show-total\n                    @on-change=\"pageChange\"\n                    :page-size